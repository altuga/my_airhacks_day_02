node {
   stage('Build') {
         sh "pwd"
         sh "cd airport/airport && mvn -DskipTests package"
   }
   stage('Build System Tests') {
         sh "cd airport/airport-st && mvn -DskipTests package"
   }
   stage('Unit Test') {
         sh "cd airport/airport && mvn test"
   }
   stage('Build Server Image') {
         sh "echo building server"
   }
   stage('Run Server') {
         sh "echo running server"
   }
   stage('Run System Tests') {
         sh "cd airport/airport-st && mvn failsafe:integration-test failsafe:verify"
   }
   stage('Promote'){
       sh "echo tag image"
   }

}